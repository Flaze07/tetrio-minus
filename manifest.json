{
  "name": "TETRIO MINUS",
  "description": "heueueue",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://tetr.io/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources" : [
    "/source/connected.js",
    "/source/map.js"
  ],
  "permissions": [
    "*://tetr.io/*",
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "res/external/128.png"
  }
}
